{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"teasers": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/teasers",
			"sourceRoot": "projects/teasers/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"ssr": true,
						"target": "teasers:esbuild:development"
					},
					"configurations": {
						"production": { },
						"development": { }
					},
					"defaultConfiguration": "development"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "teasers:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "teasers:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/teasers/tsconfig.spec.json",
						"assets": [
							"projects/teasers/src/favicon.ico",
							"projects/teasers/src/assets"
						],
						"styles": [
							"projects/teasers/src/styles.scss"
						],
						"scripts": []
					}
				},
				"esbuild": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/teasers",
						"index": "projects/teasers/src/index.html",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/teasers/tsconfig.app.json",
						"assets": [
							"projects/teasers/src/favicon.ico",
							"projects/teasers/src/assets"
						],
						"styles": [
							"projects/teasers/src/styles.scss"
						],
						"scripts": [],
						"browser": "projects/teasers/src/main.ts",
						"server": "projects/teasers/src/main.server.ts",
						"prerender": true,
						"optimization": false,
						"sourceMap": true,
						"namedChunks": true,
						"ssr": {
							"entry": "projects/teasers/server.ts"
						}
					},
					"configurations": {
						"production": {

						},
						"development": {

						}
					},
					"defaultConfiguration": "development"
				},
				"serve-original": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "teasers:esbuild:production"
						},
						"development": {
							"buildTarget": "teasers:esbuild:development",
							"port": 4201
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201
					}
				}
			}
		},
		"recommendations": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/recommendations",
			"sourceRoot": "projects/recommendations/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {},
					"configurations": {
						"production": {
							"target": "recommendations:esbuild:production"
						},
						"development": {
							"target": "recommendations:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "recommendations:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "recommendations:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/recommendations/tsconfig.spec.json",
						"assets": [
							"projects/recommendations/src/favicon.ico",
							"projects/recommendations/src/assets"
						],
						"styles": [
							"projects/recommendations/src/styles.scss"
						],
						"scripts": []
					}
				},
				"esbuild": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/recommendations",
						"index": "projects/recommendations/src/index.html",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/recommendations/tsconfig.app.json",
						"assets": [
							"projects/recommendations/src/favicon.ico",
							"projects/recommendations/src/assets"
						],
						"styles": [
							"projects/recommendations/src/styles.scss"
						],
						"scripts": [],
						"browser": "projects/recommendations/src/main.ts",
						"server": "projects/recommendations/src/main.server.ts",
						"prerender": true,
						"ssr": {
							"entry": "projects/recommendations/server.ts"
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "recommendations:esbuild:production"
						},
						"development": {
							"buildTarget": "recommendations:esbuild:development",
							"port": 4202
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4202
					}
				}
			}
		}
	}
}