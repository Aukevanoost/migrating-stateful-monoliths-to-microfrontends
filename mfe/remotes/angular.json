{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"teasers": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/teasers",
			"sourceRoot": "projects/teasers/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"ssr": true,
						"target": "teasers:esbuild:development"
					},
					"configurations": {
						"production": { },
						"development": { }
					},
					"defaultConfiguration": "development"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "teasers:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "teasers:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/teasers/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/teasers/src/assets"
						],
						"styles": [
							"projects/teasers/src/styles.scss"
						],
						"scripts": []
					}
				},
				"esbuild": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/teasers",
						"index": "projects/teasers/src/index.html",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/teasers/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/teasers/src/assets"
						],
						"styles": [
							"projects/teasers/src/styles.scss"
						],
						"scripts": [],
						"browser": "projects/teasers/src/main.ts",
						"server": "projects/teasers/src/main.server.ts",
						"prerender": false,
						"optimization": false,
						"sourceMap": true,
						"namedChunks": true,
						"ssr": {
							"entry": "projects/teasers/server.ts"
						}
					},
					"configurations": {
						"production": {

						},
						"development": {

						}
					},
					"defaultConfiguration": "development"
				},
				"serve-original": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "teasers:esbuild:production"
						},
						"development": {
							"buildTarget": "teasers:esbuild:development",
							"port": 4001
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4001
					}
				}
			}
		},
		"recommendations": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/recommendations",
			"sourceRoot": "projects/recommendations/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {},
					"configurations": {
						"production": {
							"target": "recommendations:esbuild:production"
						},
						"development": {
							"target": "recommendations:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "recommendations:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "recommendations:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/recommendations/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/recommendations/src/assets"
						],
						"styles": [
							"projects/recommendations/src/styles.scss"
						],
						"scripts": []
					}
				},
				"esbuild": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/recommendations",
						"index": "projects/recommendations/src/index.html",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/recommendations/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/recommendations/src/assets"
						],
						"styles": [
							"projects/recommendations/src/styles.scss"
						],
						"scripts": [],
						"browser": "projects/recommendations/src/main.ts",
						"server": "projects/recommendations/src/main.server.ts",
						"prerender": true,
						"ssr": {
							"entry": "projects/recommendations/server.ts"
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "recommendations:esbuild:production"
						},
						"development": {
							"buildTarget": "recommendations:esbuild:development",
							"port": 4002
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4002
					}
				}
			}
		},
		"homepage": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/homepage",
			"sourceRoot": "projects/homepage/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"ssr": true,
						"target": "homepage:esbuild:development"
					},
					"configurations": {
						"production": {
						},
						"development": {
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "homepage:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/homepage/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/homepage/public"
							}
						],
						"styles": [
							"projects/homepage/src/styles.scss"
						],
						"scripts": []
					}
				},
				"esbuild": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/homepage",
						"index": "projects/homepage/src/index.html",
						"browser": "projects/homepage/src/main.ts",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/homepage/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/homepage/public"
							}
						],
						"styles": [
							"projects/homepage/src/styles.scss"
						],
						"scripts": [],
						"baseHref": "http://localhost:4000/",
						"server": "projects/homepage/src/main.server.ts",
						"prerender": false,
						"ssr": {
							"entry": "projects/homepage/server.ts"
						}
					},
					"configurations": {
						"production": {
							"optimization": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kB",
									"maximumError": "4kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "homepage:esbuild:production"
						},
						"development": {
							"buildTarget": "homepage:esbuild:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4000
					}
				}
			}
    },
    "shared": {
      "projectType": "library",
      "root": "projects/shared",
      "sourceRoot": "projects/shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shared/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
	}
}